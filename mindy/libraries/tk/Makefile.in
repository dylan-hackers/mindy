##  This is the makefile for the Mindy/Tk windowing library.
##

SHELL	=	/bin/sh

##
## begin autoconf'ed source
##

prefix	=		@prefix@
exec_prefix	=	@exec_prefix@
srcdir	=		@srcdir@

SRCDIR	=		${srcdir}
BINDIR	=		${exec_prefix}/bin
LIBDIR	=		${exec_prefix}/lib/mindy

CC	=		@CC@
CCOPTS	=		@CCOPTS@
CFLAGS	=		$(CCOPTS) -I${SRCDIR} @DEFS@ -DBINDIR=\"${BINDIR}\" -DLIBDIR=\"${LIBDIR}\"
YACC	=		@YACC@
YFLAGS	=		-d
LEX	=		@LEX@
LFLAGS	=		@LFLAGS@
RANLIB	=		@RANLIB@
INSTALL	=		@INSTALL@
INSTALL_PROGRAM	=	@INSTALL_PROGRAM@
INSTALL_DATA	=	@INSTALL_DATA@

LIBOBJS =		@LIBOBJS@
LIBS	=		@LIBS@

##
## end of configure written portion
##

MC	= ../../comp/mindycomp

MINDYFLAGS = -lTk

TKOBJS = tk-lib.dbc tk-io.dbc tk-call.dbc tk-util.dbc tk-window.dbc \
         tk-button.dbc tk-editable.dbc tk-listbox.dbc tk-scrollbar.dbc \
         tk-frame.dbc tk-toplevel.dbc tk-label.dbc tk-message.dbc \
         tk-scale.dbc tk-entry.dbc tk-menu.dbc tk-text.dbc tk-canvas.dbc \
         tk-bind.dbc tk-winfo.dbc tk-class.dbc

tk.dbc: ${TKOBJS}
	cat ${TKOBJS} > $@

tk-lib.dbc: ${SRCDIR}/tk-lib.dylan
	${MC} ${MINDYFLAGS} -o tk-lib.dbc ${SRCDIR}/tk-lib.dylan
tk-util.dbc: ${SRCDIR}/tk-util.dylan
	${MC} ${MINDYFLAGS} -o tk-util.dbc ${SRCDIR}/tk-util.dylan
tk-window.dbc: ${SRCDIR}/tk-window.dylan
	${MC} ${MINDYFLAGS} -o tk-window.dbc ${SRCDIR}/tk-window.dylan
tk-call.dbc: ${SRCDIR}/tk-call.dylan
	${MC} ${MINDYFLAGS} -o tk-call.dbc ${SRCDIR}/tk-call.dylan
tk-io.dbc: ${SRCDIR}/tk-io.dylan
	${MC} ${MINDYFLAGS} -o tk-io.dbc ${SRCDIR}/tk-io.dylan
tk-button.dbc: ${SRCDIR}/tk-button.dylan
	${MC} ${MINDYFLAGS} -o tk-button.dbc ${SRCDIR}/tk-button.dylan
tk-editable.dbc: ${SRCDIR}/tk-editable.dylan
	${MC} ${MINDYFLAGS} -o tk-editable.dbc ${SRCDIR}/tk-editable.dylan
tk-listbox.dbc: ${SRCDIR}/tk-listbox.dylan
	${MC} ${MINDYFLAGS} -o tk-listbox.dbc ${SRCDIR}/tk-listbox.dylan
tk-scrollbar.dbc: ${SRCDIR}/tk-scrollbar.dylan
	${MC} ${MINDYFLAGS} -o tk-scrollbar.dbc ${SRCDIR}/tk-scrollbar.dylan
tk-frame.dbc: ${SRCDIR}/tk-frame.dylan
	${MC} ${MINDYFLAGS} -o tk-frame.dbc ${SRCDIR}/tk-frame.dylan
tk-toplevel.dbc: ${SRCDIR}/tk-toplevel.dylan
	${MC} ${MINDYFLAGS} -o tk-toplevel.dbc ${SRCDIR}/tk-toplevel.dylan
tk-label.dbc: ${SRCDIR}/tk-label.dylan
	${MC} ${MINDYFLAGS} -o tk-label.dbc ${SRCDIR}/tk-label.dylan
tk-message.dbc: ${SRCDIR}/tk-message.dylan
	${MC} ${MINDYFLAGS} -o tk-message.dbc ${SRCDIR}/tk-message.dylan
tk-scale.dbc: ${SRCDIR}/tk-scale.dylan
	${MC} ${MINDYFLAGS} -o tk-scale.dbc ${SRCDIR}/tk-scale.dylan
tk-entry.dbc: ${SRCDIR}/tk-entry.dylan
	${MC} ${MINDYFLAGS} -o tk-entry.dbc ${SRCDIR}/tk-entry.dylan
tk-menu.dbc: ${SRCDIR}/tk-menu.dylan
	${MC} ${MINDYFLAGS} -o tk-menu.dbc ${SRCDIR}/tk-menu.dylan
tk-text.dbc: ${SRCDIR}/tk-text.dylan
	${MC} ${MINDYFLAGS} -o tk-text.dbc ${SRCDIR}/tk-text.dylan
tk-canvas.dbc: ${SRCDIR}/tk-canvas.dylan
	${MC} ${MINDYFLAGS} -o tk-canvas.dbc ${SRCDIR}/tk-canvas.dylan
tk-bind.dbc: ${SRCDIR}/tk-bind.dylan
	${MC} ${MINDYFLAGS} -o tk-bind.dbc ${SRCDIR}/tk-bind.dylan
tk-winfo.dbc: ${SRCDIR}/tk-winfo.dylan
	${MC} ${MINDYFLAGS} -o tk-winfo.dbc ${SRCDIR}/tk-winfo.dylan
tk-class.dbc: ${SRCDIR}/tk-class.dylan
	${MC} ${MINDYFLAGS} -o tk-class.dbc ${SRCDIR}/tk-class.dylan

clean:
	rm -f ${TKOBJS} tk.dbc dylan-tk.html

realclean:
	rm -f ${TKOBJS} tk.dbc Makefile *~ \#* core

install: tk.dbc
	${INSTALL_DATA} tk.dbc ${LIBDIR}
